;; 
;; This is the main configuration file for Grid Mode
;;
[Includes]
    ;; Just normal grid mode
    Include-Grid = Configuration/Grid/Grid.ini

[Configuration]
    GridServerURI = "http://127.0.0.1:8012/grid/"
	
	;; If regions should be marked as offline on the map
	;;  instead of being removed on shutdown.
	MarkRegionsAsOffline = true
	
	;; For setting a password for this region
    ;RegisterRegionPassword = 
